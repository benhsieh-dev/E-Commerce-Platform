{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "GraphQL": "Information"
    }
  },
  "AllowedHosts": "*",
  "Services": {
    "UserService": "https://localhost:5001",
    "PropertyService": "https://localhost:5002",
    "BookingService": "https://localhost:5003",
    "PaymentService": "https://localhost:5004",
    "ReviewService": "https://localhost:5005",
    "NotificationService": "https://localhost:5006"
  },
  "Jwt": {
    "Key": "your-super-secret-jwt-key-that-is-at-least-256-bits-long-for-production",
    "Issuer": "AirBnbPlatform",
    "Audience": "AirBnbPlatformUsers",
    "ExpiryMinutes": 1440
  },
  "GraphQL": {
    "EnableMetrics": true,
    "EnableIntrospection": true,
    "MaxDepth": 10,
    "MaxComplexity": 1000,
    "EnablePersistQueries": true
  },
  "RateLimiting": {
    "EnableRateLimiting": true,
    "RequestsPerMinute": 1000,
    "RequestsPerHour": 10000
  },
  "Caching": {
    "EnableResponseCaching": true,
    "DefaultCacheDurationMinutes": 5,
    "QueryCacheDurationMinutes": 10
  }
}